{
  "name": "angular-matecu-project",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "compile:sass": "node-sass projects/angular-matecu/src/scss/main.scss projects/angular-matecu/src/css/main.css --output-style compressed",
