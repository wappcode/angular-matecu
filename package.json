{
  "name": "angular-matecu-project",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "compile:sass": "node-sass projects/angular-matecu/src/scss/main.scss projects/angular-matecu/src/css/main.css --output-style compressed",
    "compile:sass-watch": "node-sass projects/angular-matecu/src/scss/main.scss projects/angular-matecu/src/css/main.css -w",
    "build:library-watch": "ng build --prod --watch angular-matecu",
    "build:library": "ng build --configuration production angular-matecu",
    "build": "npm-run-all compile:sass build:library",
    "build-watch": "npm-run-all --parallel compile:sass-watch build:library-watch",
