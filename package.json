{
  "name": "angular-matecu-project",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:library-watch": "ng build --prod --watch angular-matecu",
    "build-library": "ng build --configuration production angular-matecu",
    "copy-changelog-dist": "copyfiles -f projects/angular-matecu/CHANGELOG.md dist/angular-matecu",
    "build:library": "npm-run-all --sequential build-library copy-changelog-dist",
    "build": "npm-run-all  build:library",
    "build-watch": "npm-run-all --parallel build:library-watch",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.5.1",
  "dependencies": {
    "@angular/cdk": "~21.1.3",
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/material": "~21.1.3",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.0",
    "@angular/cli": "^21.1.0",
    "@angular/compiler-cli": "^21.1.0",
    "@angular/material": "^21.1.3",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
